<%- include('includes/header.ejs') %>
	<%- include('includes/nav.ejs') %>
		<!-- main -->
		<div class="main">
			<div class="container">
				<div class="container bg-light my-3">
					<h4 class="mx-3 pt-4"><a href="/asset_files">Add assets</a></h4>
					<% if(advisory.length> 0) { %>
						<h2 class="my-4 mx-3">
							Advisory
						</h2>
						<div class="pb-4 my-4 mx-3">
							<hr>
							<% for(let items of advisory ) { %>
								<div class="row">
									<div class="col-4"><a href="/edit/<%=items.type %>">
											<%=items.name %>
										</a></div>
									<div class="col-4"><a target="_blank" href="<%=items.file %>"><%= items.file_name %></a></div>
									<div class="col-4"><spna><%= items.email || items.institution %></spna></div>
								</div>
								<hr>
								<% } %>
						</div>
					<% } %>
				</div>
				<div class="container bg-light">
					<h2 class="my- mx-3">Timeline</h2>
				</div>
			</div>
		</div>
		</div>
		</div>
		<%- include('includes/footer.ejs') %>